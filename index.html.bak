    <script type="text/javascript"> 
         // 추천상품
        $(function(){ //$(document).ready(function () { 과 동일
            // 현재번호 / 총갯수
            $(".tleft").prepend("<span class='count'><strong class='current'></strong><strong class='all'></strong> </span>");
            var sum = $(".tslide .slick-dots li").length; //length는 요소의 갯수
            $(".tleft .all").text("0"+sum);
            
            var cnumber;
            cnumber = $(".tslide .slick-dots .slick-active").index()+1; //index()는 0부터 시작됨
            console.log(cnumber);
            $(".tleft .current").text("0"+cnumber); //
            
            /* 슬라이드항목 설명글 이동하기 */
            $("#title .wrap").append("<div class='wtleft'></div>");
            $('#title .tleft').appendTo(".wtleft");
            $(".wtleft div.tleft"+cnumber).addClass("tleftShow");;

            setInterval(function(){
                cnumber = $(".tslide .slick-dots .slick-active").index()+1;
                $(".tleft .current").text("0"+cnumber);
                $(".wtleft div.tleft"+cnumber).addClass("tleftShow");
                $(".wtleft div.tleft").not(".wtleft div.tleft"+cnumber).removeClass("tleftShow");
                $("#title .slick-initialized .slick-slide.slick-current").removeClass("tlefthide");
                $("#title .slick-initialized .slick-slide").not(".slick-current").addClass("tlefthide");
            },2000);
            setInterval(function(){
                 
            },4000);


            // $('.tslide .slick-dots li').click(function(){ 
            //     var indexNum = $(this).index(); 
            //     alert('나는'+indexNum+'번째 자식입니다'); 
            // })

           /* 추천상품 more버튼 이동하기 */
           $(".product").before($("#recommand .more"));
            
        });
    </script>
