    <script type="text/javascript">
        $(function(){
            // 인기제품
           $("#bestproduct .list li:gt(2)").hide();
            var a=0;
            
           $("#bestproduct .more a").click(function(){
              $("#bestproduct .list li:gt(2)").slideToggle();
              var more=$("#bestproduct .list li:eq(3)").offset().top;
              $("html,body").animate({scrollTop:more},1500);
